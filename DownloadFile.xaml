<Window x:Class="PistachoStudiosLauncherWPF.DownloadFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PistachoStudiosLauncherWPF"
        mc:Ignorable="d"
        Title="Actualizador de Pistacho Studios" Height="96" Width="394" ResizeMode="NoResize" Cursor="Wait" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="11*"/>
        </Grid.RowDefinitions>
        <ProgressBar Grid.Row="1" x:Name="pbprogress" HorizontalAlignment="Center" Height="27" VerticalAlignment="Top" Width="374" IsIndeterminate="True" Margin="0,6,0,0"/>
        <Label Content="Actualizando el launcher..." HorizontalAlignment="Left" Height="23" VerticalAlignment="Center" Width="149"/>

    </Grid>
</Window>
